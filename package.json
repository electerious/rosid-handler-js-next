{
  "name": "rosid-handler-js-next",
  "version": "1.0.1",
  "description": "Load, transform, bundle and compress modern JS",
  "keywords": [
    "rosid",
    "handler",
    "transform",
    "compile",
    "js",
    "babel",
    "rollup",
    "terser"
  ],
  "homepage": "https://github.com/electerious/rosid-handler-js-next",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/rosid-handler-js-next.git"
  },
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "eslint": "eslint \"**/*.js\"",
    "format": "npm run eslint -- --fix && npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,md,yml}\"",
    "test": "npm run lint && node --test"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.53.2",
    "rollup-plugin-node-globals": "^1.4.0"
  },
  "devDependencies": {
    "@electerious/eslint-config": "^5.2.1",
    "@electerious/prettier-config": "^4.0.0",
    "eslint": "^9.39.1",
    "fsify": "^6.0.1",
    "prettier": "^3.6.2"
  },
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ]
}
